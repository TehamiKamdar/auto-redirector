{
  "manifest_version" : 3,
  "name": "Auto Redirector",
  "version": "1.1",
  "description": "Automatically redirects specified URLs with a toggle and custom destination input.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background":{
    "service_worker":"background.js"
  },
  "action":{
    "default_popup":"popup.html",
    "default_title": "Auto Redirector (Youtube Only)"
  }
}