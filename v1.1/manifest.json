{
  "manifest_version" : 3,
  "name": "Auto Redirector",
  "version": "1.1",
  "description": "Automatically redirects specified URLs with a toggle and custom destination input.",
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background":{
    "service_worker":"background.js"
  },
  "action":{
    "default_popup":"index.html",
    "default_icon":{
      "16": "",
      "48": "",
      "128": ""
    }
  },
  "icons":{
    "16": "",
    "48": "",
    "128": ""
  }
}